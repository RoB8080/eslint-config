{
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "strict": true,
        "isolatedModules": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "baseUrl": ".",
        "paths": {
        },
        "lib": [
            "esnext",
            "scripthost"
        ]
    },
    "exclude": [
        "node_modules"
    ]
}
